    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^10.12.16",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.4.3",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "^13.4.3",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
